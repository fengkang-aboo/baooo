}
.headerUser .userinfo .avatarwrp:after{
	content: ' ';
    background-size: 20px 6px;
    background-position: 0px 0px;
    width: 10px;
    height: 6px;
    background-image: url(../img/ic_updown.png);
    position: absolute;
    right: -20px;
    top: 15px;
    background-repeat: no-repeat;
}
.headerUser .userinfo.on .avatarwrp:after{
    background-position: -10px 0px;
}
.headerUser .userinfo .dropmenu {
	display: none;
	position: absolute;
	top: 56px;
    right: 13px;
	z-index: 100;
	width: 178px;
	background-color: #fff;
	background-color: rgba(253,253,253,.9);
	border: 1px solid #DFDFDF;
	border: 1px solid rgba(0,0,0,.12);
	border-radius: 3px;
	-webkit-box-shadow: 1px 1px 3px rgba(0,0,0,.1);
	-moz-box-shadow: 1px 1px 3px rgba(0,0,0,.1);
	-box-shadow: 1px 1px 3px rgba(0,0,0,.1)
}

.headerUser .userinfo.on .dropmenu{
	display: block;
    -webkit-animation: show-menu .1s ease-in;
    animation: show-menu .1s ease-in;
}
.headerUser .userinfo .dropmenu li {
	height: 32px;
	text-align: left;
	vertical-align: top
}

.headerUser .userinfo .dropmenu a {
	display: block;
	padding: 0 12px;
	line-height: 32px;
	font-size: 14px;
	color: #666
}

.headerUser .userinfo .dropmenu .username {
	background-color: #f4f4f4;
	border-bottom: 1px solid #D7D7D7;
	border-bottom: 1px solid rgba(0,0,0,.12)
}

.headerUser .userinfo .dropmenu .username a {
	max-width: 100%;
	overflow: hidden;
	white-space: nowrap
}

.headerUser .userinfo .dropmenu .has-update a {
	color: #e34a4a
}

.headerUser .userinfo .dropmenu .inner-arrow,.headerUser .userinfo .dropmenu .outer-arrow {
	position: absolute;
	width: 0;
	height: 0;
	font-size: 0;
	color: transparent;
	border-width: 10px;
	border-style: solid
}

.headerUser .userinfo .dropmenu .outer-arrow {
	top: -20px;
	right: 36px;
	border-color: transparent transparent #DFDFDF
}

.headerUser .userinfo .dropmenu .inner-arrow {
	top: -9px;
	right: -10px;
	border-color: transparent transparent #F4F4F4
}

@-webkit-keyframes show-menu {
	0% {
		-webkit-transform: translateY(-8px);
		-moz-transform: translateY(-8px);
		transform: translateY(-8px)
	}

	100% {
		-webkit-transform: translateY(0);
		-moz-transform: translateY(0);
		transform: translateY(0)
	}
}

@-moz-keyframes show-menu {
	0% {
		-webkit-transform: translateY(-8px);
		-moz-transform: translateY(-8px);
		transform: translateY(-8px)
	}

	100% {
		-webkit-transform: translateY(0);
		-moz-transform: translateY(0);
		transform: translateY(0)
	}
}

