/* ----------------------------------
 * bodynavs
 * ---------------------------------- */
.bodynavs{
	width: 100%;
	height: 45px;
	line-height: 45px;
	background-color: #fff;
	overflow: hidden;
    border-bottom: 1px solid #c8c8c8;
}
.bodynavs .wrp{
	overflow-x: scroll;
    overflow-y: hidden;
	white-space: nowrap;
}
.bodynavs .item{
	height: 30px;
	line-height: 30px;
	background-color: #eeeeee;
	font-size: 15px;
	color: #a8a8a8;
    padding: 0px 20px;
    border-radius: 15px;
    margin: 7px;
    display: inline-block;
}
.bodynavs .item.active{
	background-color: #f2a501;
	color: #fff;
}
.bodynavs .row .tag{
	display: inline-block;
    border-radius: 12px;
    background-color: #fff;
    line-height: 21px;
    padding: 0px 8px;
    float: right;
    margin: 14px 26px 9px 9px;
}
.bodynavs .row.active .tag{
	color: #fff;
	background-color: #f2a501;
}
.bodynavs .row.title{
	height: 35px;
	line-height: 35px;
	background-color: #d9e2eb;
}

/* ----------------------------------
 * slidenavs
 * ---------------------------------- */
.slidenavs{
	display: block;
	width: 100%;
	height: 45px;
	background-color: #d9e2eb;
	overflow: hidden;
    z-index: 9;
}
.slidenavs .menu{
	display: none;
	position: absolute;
	top: 45px;
	left: 0px;
	width: 100%;
    z-index: 9;
    overflow: hidden;
}
.slidenavs.on .menu{
	display: block;
}
.slidenavs .row{
	height: 45px;
	line-height: 45px;
	background-color: #fff;
	border-bottom: 1px solid #c8c8c8;
	font-size: 15px;
	color: #000;
	position: relative;
}
.slidenavs .slidebtn{
	background-color: #fff;
	color: #f2a501;
	text-align: center;
}
.slidenavs .slidebtn i{
	width: 12.5px;
	height: 6.5px;
	background-size: 12.5px 6.5px;
	background-repeat: no-repeat;
	background-image: url(../img/ic_down.png);
    display: inline-block;
    margin-left: 8px;
}
.slidenavs.on .slidebtn i{
	background-image: url(../img/ic_up.png);
}
.slidenavs .row.active:after{
	content: " ";
    position: absolute;
    right: 30px;
    top: 17px;
    background-repeat: no-repeat;
    background-image: url(../img/ic_ok.png);
    background-size: 18px 11px;
    width: 18px;
    height: 11px;
    z-index: 28;
    visibility: visible;
}
.slidenavs .row .col-md-12{
	padding-left: 38px;
}
.slidenavs .row .col-md-12 a{
	display: block;
}	
.slidenavs .row .icon{
	display: inline-block;
	width: 25px;
	height: 25px;
	margin-bottom: -6px;
	margin-right: 9px;
	border-radius: 100%;
}
.slidenavs .row .tag{
	display: inline-block;
    border-radius: 12px;
    background-color: #fff;
    line-height: 21px;
    padding: 0px 8px;
    float: left;
    margin: 14px 26px 9px 9px;
}
.slidenavs .row.active .tag{
	color: #fff;
	background-color: #f2a501;
}
.slidenavs .row.title{
	height: 35px;
	line-height: 35px;
	color: #999999;
	background-color: #eeeeee;
}

